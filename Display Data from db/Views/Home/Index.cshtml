@{
    ViewData["Title"] = "Display Data From DB";
    string[] TableHeaders = new string[] {"Address ID"
      ,"Address Line"      
      ,"City"
      ,"State Province ID"
      ,"Postal Code"
      ,"Spatial Location"      
      ,"Row Id"
      ,"Modified Date"};
}

<div class="">
  <table id="myTable" class="table table-bordered table-hover table-sm">
    <thead>
        <tr>
            @{
                foreach  (var head in TableHeaders)
                {
                    <th>
                        @head
                    </th>
                }

            }

        </tr>
    </thead>
  
    <tbody>
        @{
            if (Model != null)
            {
                foreach (var Data in Model)
                {
                    <tr>
                        <td> @Data.AddressID </td>
                        <td> @Data.AddresLine </td>
                        <td> @Data.City </td>
                        <td> @Data.StateProviceID </td>
                        <td> @Data.PostalCode </td>
                        <td> @Data.SpatialLocation </td>
                        <td> @Data.RowID </td>
                        <td> @Data.ModifiedDate </td>
                    </tr>
                }

            }
        }
    </tbody>
    </table>
</div>
